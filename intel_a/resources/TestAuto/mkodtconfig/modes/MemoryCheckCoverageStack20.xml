<?xml version="1.0" ?>
<MODE description="Mode with MemoryCheck and Coverage">
<MKODTOPTIONS jobs="1" pertinence="no" updateinit="no" debuggermode="MemoryCheck" />
<MEMORYCHECK value="yes" stackdepth="20" />
<COVERAGE value="yes" />
<MKODTENV>
<PARAMETER name="ODT_MAX_TIME" value="60" />
<PARAMETER name="ODT_MAX_TIME_COEF" value="150" />
<PARAMETER name="MemoryCheckCoverage" value="1" />
</MKODTENV>
</MODE>

