<?xml version="1.0"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>refline</Title>
            <Description>MTL statement to declare a reference line</Description>
            <Shortcut>refline</Shortcut>
        </Header>
        <Snippet>
            <Code Language="dsmtl">
                <![CDATA[<@ refline @>]]>
            </Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
