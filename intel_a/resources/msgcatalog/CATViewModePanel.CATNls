Title="View Mode Customization";
//-----------------------------------------------------------------------------
// Edges frame
//-----------------------------------------------------------------------------
mainFrame.optionFrame.edgeOptionFrame.HeaderFrame.Global.Title="Lines and points";

//--------------------------------------
// Edges
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeFrame.edgeBtn.Title="Edges and points";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeFrame.edgeBtn.LongHelp="Displays the geometry in Shading mode with points and edges.";

//--------------------------------------
// All Edges
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.allEdgeBtn.Title="All edges";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.allEdgeBtn.LongHelp="Displays the geometry with all edges, whatever their type (smooth or not).
This mode is equivalent to the Shading with Edges mode available in the View toolbar.";


//--------------------------------------
//  withHalfLightedSmoothEdges
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.halfLightedSmoothEdgeBtn.Title="Half visible smooth edges";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.halfLightedSmoothEdgeBtn.LongHelp="Partially displays smooth edges which are drawn in such
a way that they are less visible than sharp edges.
This option has no impact if the Shading option is cleared.";

//--------------------------------------
// WithoutSmoothEdges 
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.withoutSmoothEdgeBtn.Title="No smooth edges";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.edgeRadioFrame.withoutSmoothEdgeBtn.LongHelp="Hides smooth edges on the geometry.
This mode is equivalent to the Shading with Edges without Smooth Edges mode available in the View toolbar.";

//--------------------------------------
//  All points
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.pointRadioFrame.allPointBtn.Title="All points";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.pointRadioFrame.allPointBtn.LongHelp="Displays edges and all their points, including vertices.";

//--------------------------------------
//  Without vertices
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.pointRadioFrame.withoutVertexBtn.Title="No vertices";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.pointRadioFrame.withoutVertexBtn.LongHelp="Deactivates the display of vertices for edges.
Only free points are displayed and can be selected.
This enhances the overall readibility for complex models
and has a positive impact on performance.";

//--------------------------------------
//  ColoredEdgesFromFaces
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.ColoredEdgesFromFacesFrame.ColoredEdgesFromFacesBtn.Title="Colored edges from faces";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.ColoredEdgesFromFacesFrame.ColoredEdgesFromFacesBtn.LongHelp="Colors edges (border edges, internal smooth
edges, or sharp edges) according to the color applied to the geometry's faces.";


//--------------------------------------
//  outline
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.outlineFrame.outlineBtn.Title="Outlines";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.outlineFrame.outlineBtn.LongHelp="Determines whether outlines are displayed or not.
This mode is especially useful when working with surfaces
of revolution (such as shafts or cylinders).
There is a price to pay in memory usage when using this mode.";

//--------------------------------------
//  line-on-line
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.lineOnLineFrame.lineOnLineBtn.Title="Line-on-line";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.lineOnLineFrame.lineOnLineBtn.LongHelp="Enables to visualize superimposed lines by displaying them in a
different color.
It is recommended to set a dark color background for better visibility.";

//--------------------------------------
// Isopar
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.isoparFrame.isoparBtn.Title="Isoparametrics";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.isoparFrame.isoparBtn.LongHelp="Displays topological elements defined as being isoparametrics.
To use this option, first select the Enable isoparametrics generation
option in Tools > Options > General > Display > Performance.";

//--------------------------------------
//  Nowire
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutwiresFrame.withoutwiresBtn.Title="No wires";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutwiresFrame.withoutwiresBtn.LongHelp="Deactivates the display of wires.";

//--------------------------------------
//  Noaxis
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutaxisFrame.withoutaxisBtn.Title="No axes";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutaxisFrame.withoutaxisBtn.LongHelp="Deactivates the display of axes and axis systems.";

//--------------------------------------
//  NoPoints
//--------------------------------------
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutpointsFrame.withoutpointsBtn.Title="No points";
mainFrame.optionFrame.edgeOptionFrame.IconAndOptionsFrame.OptionsFrame.withoutpointsFrame.withoutpointsBtn.LongHelp="Deactivates the display of points.";

//-----------------------------------------------------------------------------
// Mesh frame
//-----------------------------------------------------------------------------
mainFrame.optionFrame.meshOptionFrame.HeaderFrame.Global.Title="Mesh";

//--------------------------------------
//  SimpleShading
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.simpleShadingFrame.simpleShadingBtn.Title="Shading";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.simpleShadingFrame.simpleShadingBtn.LongHelp="Defines the type of shading to be used for displaying the geometry.";

//--------------------------------------
//  Gouraud
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.shadingFrame.shadingBtn.Title="Gouraud";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.shadingFrame.shadingBtn.LongHelp="Displays the geometry in Gouraud shading.
This surface-shading technique linearly interpolates a color or a shade across a
polygon to make the geometry appear smoother.
This mode is the default visualization mode and is equivalent to the Shading mode
available in the View toolbar.";

//--------------------------------------
// Material 
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.materialFrame.materialBtn.Title="Material";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.materialFrame.materialBtn.LongHelp="Displays the materials applied onto the geometry.
This mode is equivalent to the Shading with Material mode available in the View toolbar.";

//--------------------------------------
// Polygons
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.polygonFrame.polygonBtn.Title="Triangles";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.polygonFrame.polygonBtn.LongHelp="Displays the triangles on the geometry's faces.";

//--------------------------------------
// Transparent 
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.transparentFrame.transparentBtn.Title="Transparent";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.shadingRadioFrame.transparentFrame.transparentBtn.LongHelp="Displays the whole geometry in transparency mode
provided that the Shading mode is activated.";

//--------------------------------------
// HiddenEdges & points 
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.hedgeFrame.hedgeBtn.Title="Hidden edges and points";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.hedgeFrame.hedgeBtn.LongHelp="Displays the geometry in Shading mode with points and edges, whether hidden or not.
This mode is equivalent to the Shading with Edges and Hidden Edges mode available in the View toolbar.";

//--------------------------------------
//  hrd
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.hrdFrame.hrdBtn.Title="Dynamic hidden line removal";
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.hrdFrame.hrdBtn.LongHelp="Displays the geometry in quick dynamic hidden line removal mode.
In this mode, 3D faces visually blank out any element located behind them,
including 1D elements (such as wireframe elements).";

//--------------------------------------
//  toon
//--------------------------------------
mainFrame.optionFrame.meshOptionFrame.IconAndOptionsFrame.OptionsFrame.toonFrame.toonBtn.Title="Toon";

//--------------------------------------
//  Options
//--------------------------------------
mainFrame.optionFrame.optionsOptionFrame.HeaderFrame.Global.Title="Options";

//--------------------------------------
//  ObjectViewMode
//--------------------------------------
mainFrame.optionFrame.optionsOptionFrame.IconAndOptionsFrame.OptionsFrame.objectViewModeFrame.objectViewModeBtn.Title="Rendering style per object";
mainFrame.optionFrame.optionsOptionFrame.IconAndOptionsFrame.OptionsFrame.objectViewModeFrame.objectViewModeBtn.LongHelp="Takes into account the rendering styles
defined in the Properties dialog box or in the Graphic Properties toolbar
when visualizing objects in a 3D window.";

