	"	[-help|-h][-version] \n",
	"	-drop -out outputFilePath\n",
	"***********************************************************\n";

mkcsJavaData.HelpEnd = " \n",
		"Java Data Converter Options:\n",
		"------------------\n",
		"  -version                     : Displays version number\n",
		"  -h                           : Displays command help (synopsis)\n",
		"  -help                        : Displays command help (detailled)\n",
		"  -pmdxml xmlFilePath          : \"xmlFilePath\" is a xml file containing\n",
		"                                 java errors data generated by PMD java\n",
		"                                 check tool.\n",
		"  -mgroup MainGroupName        : Main group name of checked sources\n",
