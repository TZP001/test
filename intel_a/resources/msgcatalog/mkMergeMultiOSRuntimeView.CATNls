mkMergeMultiOSRuntimeView.Version.BuildLevel	= "\n/P1 (Build /P2)";

mkMergeMultiOSRuntimeView.HelpBeginWindows	= "Merge of Multi Operating system run time view.\n",
"\nUsage: mkMergeMultiOSRuntimeView [-W Workspace] [-p Path1[;Path2]] [-o OS [-o OS]] [-h|-?] [-v] -lFW List | Framework\n";

mkMergeMultiOSRuntimeView.HelpBeginUnix		= "\n",
    "Merge of Multi Operating system run time view.\n",
    "\nUsage: mkMergeMultiOSRuntimeView [-W WorkspacePath] [-p Path1[:Path2]] [-o OS [-o OS]] [-h|-?] [-v] -lFW List | Framework\n";

mkMergeMultiOSRuntimeView.Option.W	="   -/P1 Workspace    : Working directory path name, default is current working directory.";
mkMergeMultiOSRuntimeView.Option.p	="   -/P1 Path1...     : A list of directories to be added to the current working directory.";
mkMergeMultiOSRuntimeView.Option.t	="   -/P1 Target       : The targeted run time view, default is the working directory sets by -W option or the default.";
mkMergeMultiOSRuntimeView.Option.o	="   -/P1 OS1 -/P1 OS2   : The list of operating systems to be merged.";
mkMergeMultiOSRuntimeView.Option.h	="   -/P1              : produces help message.";
mkMergeMultiOSRuntimeView.Option.v	="   -/P1              : verbose mode.";
mkMergeMultiOSRuntimeView.Option.lFW="   -/P1 List       : path of the list of framework to take into account.";
mkMergeMultiOSRuntimeView.Option.h.other="   -/P1              : same as -h.";

mkMergeMultiOSRuntimeView.HelpEnd = "   FW1 FW2 ...     : The frameworks to consider.\n";

mkMergeMultiOSRuntimeView.BadOptErr		="# mkmk-ERROR: Command line -> Syntax error.";

mkMergeMultiOSRuntimeView.NotMultiOS	="# mkmk-INFO: /P1: Object path name does not contain the operating system, ignored.";
mkMergeMultiOSRuntimeView.CannotLoadErr="# mkmk-ERROR: /P1: Failed to load file.";
mkMergeMultiOSRuntimeView.CannotFindErr="# mkmk-ERROR: /P1: Unable to find file in worskpaces concatenation.";
mkMergeMultiOSRuntimeView.NoFWErr		="# mkmk-ERROR: Command line: Specify at least one framework to work on.";
mkMergeMultiOSRuntimeView.NoOSErr		="# mkmk-ERROR: Command line: Specify at least two operating systems to merge.";
