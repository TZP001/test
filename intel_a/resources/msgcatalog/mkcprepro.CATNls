mkcprepro.Version.BuildLevel = "/P1 (Build /P2)";

mkcprepro.HelpBegin = "\n",
   "\nmkmk internal C/C++ language preprocessor.\n\n",
   "Usage: mkcprepro [-h, -?, -help] [-v] [-lvl CATIAV5Level.h] [-E | -P] [-D<name>[=<token>]] filename [filename [...]]\n";

mkcprepro.Option.lvl = "   -/P1              : The name of the file which defines the level.";
mkcprepro.Option.P  = "   -/P1                : Output to .i files.";
mkcprepro.Option.E =  "   -/P1                : Output to standard output, default.";
mkcprepro.Option.D =  "   -/P1<name>[=<token>]: Define preprocessor symbol <name> with <token>.";
mkcprepro.Option.h =   "   -/P1                : This help";
mkcprepro.Option.help ="   -/P1             : The same as -h";
mkcprepro.Option.v =   "   -/P1                : Activates the verbose mode";
mkcprepro.Option.h.other = "   -/P1                : The same as -h";

mkcprepro.HelpEnd = "\n",
   "All other options are ignored for compatibility with C/C++ compiler options.\n",
	"The '#include' statements are not interpreted, and the symbol definitions are not substituted.\n";
