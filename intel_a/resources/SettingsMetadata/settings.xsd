		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:pattern value="([^\*]*)|(\*[^\*]*)|(([^\*]*)\*)|(\*([^\*]*)\*)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TabpageNameType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(/(\w|\s|&amp;|\-|&#x5F;)*)*"/>
		</xsd:restriction>
	</xsd:simpleType>
