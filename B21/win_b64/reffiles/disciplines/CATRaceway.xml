    <!-- *************** Option=0 Only display Tube/Pipe/Duct/Bendable if compatible to neighbor parts - Tools/Options LOCKED *************** -->
    <!-- *************** Option=1 Display Tube/Pipe/Duct/Bendable even if NOT compatible to neighbor parts - Tools/Options LOCKED *********** -->
    <!-- *************** Option=2 Option is UNLOCKED - Default is from previous setting saved in CATSetting *********************************** -->  
    <Resource Name="IgnoreNeighborCompatibility" Description="Ignore compatibility with neighbor parts">
      <ID  Type="Misc" Driver="File" Location="2"/>
    </Resource>

    <!-- *************** Option=0 Connect Tube/Pipe/Duct/Bendable only if compatible to neighbor parts - Tools/Options LOCKED *************** -->
    <!-- *************** Option=1 Connect Tube/Pipe/Duct/Bendable even if NOT compatible to neighbor parts - Tools/Options LOCKED *********** -->
    <!-- *************** Option=2 Option is UNLOCKED - Default is from previous setting saved in CATSetting *********************************** -->
    <Resource Name="ConnectToInCompatibleNeighbor" Description="Connect part to incompatible neighbor parts">
      <ID  Type="Misc" Driver="File" Location="2"/>
    </Resource>

    <!-- *************** Option=0 Do not display a warning message during Spec-driven part placement *************** -->
    <!-- ***************          if the Spec of the part is different from the Spec of the Line     *************** -->
    <!-- *************** Option=1 Display a warning message during Spec-driven part placement        *************** -->
    <!-- ***************          if the Spec of the part is different from the Spec of the Line     *************** -->
    <Resource Name="OutOfSpecWarning" Description="Out of Spec warning message display">
      <ID  Type="Misc" Driver="File" Location="0"/>
    </Resource>

    <!-- *************** Option=1 Function Driven only                    *************** -->
    <!-- *************** Option=2 Functionless, SchematicDriven = 0       *************** -->
    <!-- *************** Option=3 Function Driven or Functionless.        *************** -->
    <Resource Name="FunctionDriven" Description="Function driven equipment part placement">
      <ID  Type="Misc" Driver="File" Location="3"/>
    </Resource>

  <!-- *************** Option=0 Do not use the Spec of the Line during Respec of parts and runs    *************** -->
    <!-- *************** Option=1 Use the Spec of the Line during Respec of parts and runs           *************** -->
    <Resource Name="AlwaysUseSpecFromLine" Description="Use Line Spec during Respec of parts and runs">
      <ID  Type="Misc" Driver="File" Location="0"/>
    </Resource>

    <!-- *************** Option=0 Use Line ID from selected run - Option is LOCKED        ****************** -->
    <!-- *************** Option=1 Do not use Line ID from selected run - Option is LOCKED ****************** -->
    <!-- *************** Option=2 Option is UNLOCKED - Default is from previous setting saved in CATSetting ** -->
  <Resource Name="GetLineFromSelection" Description="Get LineID from selected run">
    <ID  Type="Misc" Driver="File" Location="2"/>
    </Resource>

    <!-- *************** Option=0 Use Nominal Size from selected run - Option is LOCKED        ************* -->
    <!-- *************** Option=1 Do not use Nominal Size from selected run - Option is LOCKED ************* -->
    <!-- *************** Option=2 Option is UNLOCKED - Default is from previous setting saved in CATSetting ** -->
    <Resource Name="GetSizeFromSelection" Description="Get Nominal Size from selected run">
      <ID  Type="Misc" Driver="File" Location="2"/>
    </Resource>

    <!-- *************** Option=0 Use Spec from selected run - Option is LOCKED        ********************* -->
    <!-- *************** Option=1 Do not use Spec from selected run - Option is LOCKED ********************* -->
    <!-- *************** Option=2 Option is UNLOCKED - Default is from previous setting saved in CATSetting ** -->
    <Resource Name="GetSpecFromSelection" Description="Get Specification from selected run">
    <ID  Type="Misc" Driver="File" Location="2"/>
    </Resource>

    <!-- *************** Option=0 Do not use dimension rule from Standard to override      *************** -->
    <!-- *************** the attributes. Design table or reference part already contain    *************** -->
    <!-- *************** the data. Currently, affected attributes are OD and WallThickness *************** -->
    <!-- *************** Option=1 Use dimension rule from Standard to retrieve and         *************** -->
    <!-- *************** override the attributes.                                          *************** -->
    <Resource Name="OverrideReferenceAttributesUsingDimensionRule" Description="Use dimension rule from Standard to override the attributes.">
      <ID  Type="Misc" Driver="File" Location="0"/>
    </Resource>

    <!-- *************** Option=0 Allow selection of connectors from any application            *************** -->
    <!-- *************** Option=1 Allow selection of connectors only for the active application *************** -->
    <Resource Name="FilterOnApplicationCtrs" Description="Filter on application connectors during routing and part placement.">
      <ID  Type="Misc" Driver="File" Location="1"/>
    </Resource>

    <!-- *************** Option=0 Raceway and Conduit application are NOT for cableway definition.************** -->
    <!-- *************** Option=1 Raceway and Conduit application are used to define cableway network. ********* -->
    <!-- Impact: branch connections are not allow, only end connection => a node point will be added at the branch location. -->
    <Resource Name="RacewayRunCablewayNetwork" Description="Raceway and Conduit applications are used to define the cableway network.">
       <ID  Type="Misc" Driver="File" Location="0"/>
    </Resource>

