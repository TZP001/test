     <!-- ******************************************************************* -->
     <!--  The following resource defines the wildcard value for the   ****** -->
     <!--  attribute Cable Category on a cableway object like hanger,  ****** -->
     <!--  Cable penetration.  When routing a cable and the rule       ****** -->
     <!--  "Match Cable Category" is enabled, cables with any          ****** -->
     <!--  Cable Category value can only be routed through hangers     ****** -->
     <!--  with the same Cable Category value.  If this resource is    ****** -->
     <!--  non-null and non-blank then it will define the wildcard     ****** -->
     <!--  value on the hanger (cableway object).  This will allow a   ****** -->
     <!--  cable with any Cable Category value to be routed through    ****** -->   
     <!--  the hanger with the wildcard value.   If the value is ""    ****** -->
     <!--  then no wildcard.                                           ****** -->
     <!-- ******************************************************************* -->
     <Resource Name="CableCategoryWildcard" Description="Cable Category wildcard value on cableway object">
         <ID  Type="Misc" Driver="File" Location=""/>
     </Resource>

  <!-- ***************************************************************** -->
  <!--  The following resource defines the behavior on setting the    ** -->
  <!--  association status for Equipment in the command               ** -->
  <!--  "Analyze Functional-Physical Integration Status"              ** -->
  <!-- ******Option=0 The default: Function is realized if it is      ** -->
  <!-- ******         associated with an object in 3D documents.      ** -->
  <!-- ******Option=1 Function is realized if an object with the      ** -->
  <!-- ******         same name in the 3D documents even it is not    ** -->
  <!-- ******         associated with an object.                      ** -->
  <!-- ***************************************************************** -->
  <Resource Name="2D3DAssociationStatusOption" Description="Function Physical Association Status option">
    <ID  Type="Misc" Driver="File" Location="0"/>
  </Resource>

  <!-- ***************************************************************** -->
  <!--  For "Electrical Cable Database importer" command  - IR29196   ** -->
  <!--  The PRM resource "TypeofECVDataToBeImported" defines the      ** -->
  <!--  behavior on type of part to be imported in EnoviaDB. This     ** -->
  <!--  setting is only for hangers/lofts and conduit/raceway runs    ** -->
  <!-- ******Option=1 Hangers + Lofts(Loft sections/pathways/spatial lofts) ** -->
  <!-- ******Option=2 Conduit and Raceway runs                        ** -->
  <!-- ******NOTE: Equipment,Electrical eqpts etc are always imported ** -->
  <!-- ****** irrespective of the flag set                            ** -->
  <!-- ***************************************************************** -->
  <Resource Name="TypeofECVDataToBeImported" Description="Type of ECV data(Hangers and Lofts OR Conduit and Raceway runs) to be imported">
    <ID  Type="Misc" Driver="File" Location="1"/>
  </Resource>

