  <!-- *************** Option=NoLimit   - No limit on part number length. *************** -->
  <!-- *************** Option=Automatic - Automatically limit part number length based on detected file environment (File, EV5, VPM). ** -->
  <!-- *************** Option=EV5       - Use the maximum part number length (40) allowed in ENOVIA V5 (including file extension.  ***** -->
  <!-- *************** Option=VPM       - Use the maximum part number length (32) allowed in ENOVIA VPM. *************** -->
  <!-- *************** Option=Integer>0 - User defined maximum length for part number *************** -->
  <Resource Name="PartNumberMaximumLength" Description="Maximum Length Allowed for Part Number">
    <ID  Type="Misc" Driver="File" Location="EV5"/>
  </Resource>

  <!-- *************** Option=NoLimit   - No limit on part file name length. *************** -->
  <!-- *************** Option=Automatic - Automatically limit part file name length based on detected file environment (File, EV5, VPM). ** -->
