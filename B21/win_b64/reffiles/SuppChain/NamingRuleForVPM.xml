<NamingRule>

   <!-- Rule that enables to manage structure exposed CATProduct -->

   <ExtractRule id="CATProduct_name">
      <Condition>
         <Predicate type="internal" id="Type" operator="=" value="CATProduct"/>
         <Predicate type="internal" id="StorageMode" operator="=" value="STRUCTURE_EXPOSED"/>
      </Condition>
      <NamingOperator>
         <Element type="internal" id="VPM.Environment" length="8" padding=" "/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Part.Part Number"/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Part.Part Version"/>
         <Element type="string" id="-Structure"/>
      </NamingOperator>
   </ExtractRule>

   <!-- Rule for CATPart & CATProduct in workpackage -->

   <ExtractRule id="Part_Document_name">
      <Condition>
         <Predicate type="internal" id="Type" operator="LIKE" value="CATP*"/>
      </Condition>
      <NamingOperator>
         <Element type="internal" id="VPM.Environment" length="8" padding=" "/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Part.Part Number"/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Part.Part Version"/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Document Name"/>
      </NamingOperator>
   </ExtractRule>

   <!-- Rule for CATIA V4 MODEL document types -->

   <ExtractRule id="CATIA_model_name">
      <Condition/>
      <NamingOperator>
         <Element type="internal" id="VPM.Environment" length="8" padding=" "/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Doc Id"/>
         <Element type="string" id="_Document"/>
      </NamingOperator>
   </ExtractRule>

   <!-- Rule for other document types -->

   <ExtractRule id="Other_Document_name">
      <Condition/>
      <NamingOperator>
         <Element type="internal" id="VPM.Environment" length="8" padding=" "/>
         <Element type="string" id="_"/>
         <Element type="internal" id="VPM.Table" length="10" padding=" "/>
         <Element type="string" id="_"/>
         <Element type="VPDM" id="Doc Id"/>
         <Element type="string" id="_Document"/>
      </NamingOperator>
   </ExtractRule>

</NamingRule>
