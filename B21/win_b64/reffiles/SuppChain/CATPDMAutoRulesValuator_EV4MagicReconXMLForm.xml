<ParametersList>
	<Parameter Name="queryName" Type="String" Alias="%CATPDMReconcileAutoRules:queryName%" Mandatory="Yes">
		<DefaultValuesList>
			<DefaultValue>%VPM1PDMCriterion:VPM1.SimpleName%</DefaultValue>
			<DefaultValue>%VPM1PDMCriterion:VPM1.Intermediate.Name%</DefaultValue>
			<DefaultValue>%VPM1PDMCriterion:VPM1.EasyQuery.Name%</DefaultValue>
		</DefaultValuesList>
		<CurrentValue>%VPM1PDMCriterion:VPM1.Intermediate.Name%</CurrentValue>
	</Parameter>	

	<Parameter Name="forcePersistency" Type="Boolean" Alias="%CATPDMReconcileAutoRules:forcePersistency%" Shown="Yes" Mandatory="Yes">
		<DefaultValue>Yes</DefaultValue>
	</Parameter>	
	<Parameter Name="forceMapping" Type="Boolean" Alias="%CATPDMReconcileAutoRules:forceMappingForVPDMMapping%" Shown="Yes" Mandatory="Yes">
		<DefaultValue>Yes</DefaultValue>
	</Parameter>
	<Parameter Name="forceRule" Type="Boolean" Alias="%CATPDMReconcileAutoRules:forceRuleSetting%" Shown="Yes" Mandatory="Yes">
		<DefaultValue>Yes</DefaultValue>
	</Parameter>

<RenderingForm>

<MainFrame>
	<Label ref="Parameter[@Name='queryName']/@Alias" Name="FrmElemLabel1"/>
	<GC A="L" W="1" H="1" X="0" Y="0" Name="FrmElemLabel1"/>
	
	<DefaultFormSelect1 ref="Parameter[@Name='queryName']/CurrentValue" Name="FrmElemInput1"/>
	<GC A="L" J="H" W="2" H="1" X="1" Y="0" Name="FrmElemInput1"/>

	<Label Name="FrmElemLabel2">%CATPDMReconcileAutoRules:vpdm%</Label>
	<GC A="L" W="1" H="1" X="0" Y="1" Name="FrmElemLabel2"/>

    <Label Name="FrmElemLabel3">%CATScmSessionSettings:ConnectedEnvName%</Label>
	<GC A="L" W="2" H="1" X="1" Y="1" Name="FrmElemLabel3"/>
	
	<CheckBox ref="Parameter[@Name='forcePersistency']" Name="FrmElemInput2"/>
	<GC A="L" W="2" H="1" X="0" Y="2" Name="FrmElemInput2"/>

	<CheckBox ref="Parameter[@Name='forceMapping']" Name="FrmElemInput3"/>
	<GC A="L" W="2" H="1" X="0" Y="3" Name="FrmElemInput3"/>

	<CheckBox ref="Parameter[@Name='forceRule']" Name="FrmElemInput4"/>
	<GC A="L" W="2" H="1" X="0" Y="4" Name="FrmElemInput4"/>

	<Label Name="FrmElemBlank"></Label>
	<GC A="L" J="H" H="1" W="1" X="0" Y="5" Name="FrmElemBlank"/>

	<Label Name="FrmElemLongHelp">%CATPDMReconcileAutoRules:MagicRecon.LongHelp%</Label>
	<GC A="L" J="H" H="2" W="4" X="0" Y="6" Name="FrmElemLongHelp"/>
	
</MainFrame>
</RenderingForm>

</ParametersList>
