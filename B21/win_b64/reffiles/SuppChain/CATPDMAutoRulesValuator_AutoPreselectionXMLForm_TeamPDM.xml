<?xml version="1.0" encoding="UTF-8"?>

<ParametersList>
	<Parameter Name="vpdm" Type="String" Alias="%CATPDMReconcileAutoRules:vpdm%" MultiValuated="No" Shown="No" Mandatory="Yes">
		<CurrentValue>%CATScmSessionSettings:CurrentEnv%</CurrentValue>
	</Parameter>
	<Parameter Name="structure" Type="Boolean" Alias="%CATPDMReconcileAutoRules:structure%" Mandatory="Yes">
		<DefaultValue>No</DefaultValue>
	</Parameter>
	<Parameter Name="queryName" Type="String" Alias="%CATPDMReconcileAutoRules:queryName%" Mandatory="Yes">
		<DefaultValuesList>
			<DefaultValue>%CATStiPDMCriterion:SmarTeam.Criterion.SilentGenericQuery%</DefaultValue>
		</DefaultValuesList>
		<CurrentValue>%CATStiPDMCriterion:SmarTeam.Criterion.SilentGenericQuery%</CurrentValue>
	</Parameter>	
	<Parameter Name="forceMapping" Type="Boolean" Alias="forceMapping" Shown="No" Mandatory="Yes">
		<CurrentValue>No</CurrentValue>
	</Parameter>
	<Parameter Name="implicitRule" Type="Boolean" Alias="%CATPDMReconcileAutoRules:implicitRule%" Mandatory="Yes">
		<DefaultValue>Yes</DefaultValue>
	</Parameter>
<RenderingForm>

<MainFrame>
	<!-- VPDM -->
	<Label Name="FrmElemLabel1">%CATPDMReconcileAutoRules:vpdm%    </Label>
	<GC A="L" W="1" H="1" X="0" Y="0" Name="FrmElemLabel1"/>
	<Label Name="FrmElemInput1">%CATScmSessionSettings:CurrentEnvName%</Label>
	<GC A="L" W="2" H="1" X="1" Y="0" Name="FrmElemInput1"/>
	
	<!-- Expose Mode -->
	<!-- 
	<Label ref="Parameter[@Name='structure']/@Alias" Name="FrmElemLabel2"/>
	<GC A="L" W="1" H="1" X="1" Y="1" Name="FrmElemLabel2"/>
	<DefaultFormInput ref="Parameter[@Name='structure']/CurrentValue" Name="FrmElemInput2"/>
	<GC A="L" W="1" H="1" X="0" Y="1" Name="FrmElemInput2"/>
	-->
	<!-- Query Name Selection -->
	<Label ref="Parameter[@Name='queryName']/@Alias" Name="FrmElemLabel3"/>
	<GC A="L" W="1" H="1" X="0" Y="2" Name="FrmElemLabel3"/>
		<DefaultFormSelect1 ref="Parameter[@Name='queryName']/CurrentValue" Name="DefaultFormSelect1"/>
	<GC A="L" W="2" H="1" X="1" Y="2" Name="DefaultFormSelect1"/>
	
	<!-- Implicit Rule -->
	<!--
	<Label ref="Parameter[@Name='implicitRule']/@Alias" Name="FrmElemLabel4"/>
	<GC A="L" W="1" H="1" X="1" Y="3" Name="FrmElemLabel4"/>
	<DefaultFormInput ref="Parameter[@Name='implicitRule']/CurrentValue" Name="FrmElemInput4"/>
	<GC A="L" W="1" H="1" X="0" Y="3" Name="FrmElemInput4"/>
	-->
	<!-- Blank -->
	<Label Name="FrmElemBlank"></Label>
	<GC A="L" J="H" H="1" W="1" X="0" Y="4" Name="FrmElemBlank"/>
	
	<!-- Help -->
	<Label Name="FrmElemLongHelp">%CATStiPDMReconcileAutoRules:AutomaticPreselection.LongHelp%</Label>
	<GC A="L" J="H" H="2" W="4" X="0" Y="5" Name="FrmElemLongHelp"/>
	
</MainFrame>
</RenderingForm>
</ParametersList>
