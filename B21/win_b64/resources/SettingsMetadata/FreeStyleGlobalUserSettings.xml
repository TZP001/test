				<st:min>1</st:min>
				<st:max>55</st:max>
				<st:default>
					<st:role>Very thin</st:role>
					<st:value>1</st:value>
				</st:default>
			</st:uint>
		</st:attribute>
		<st:attribute Name="OutOfToleranceColor" alias="OutOfToleranceColor" Tabpage="/Shape/FreeStyle/Tolerance">
