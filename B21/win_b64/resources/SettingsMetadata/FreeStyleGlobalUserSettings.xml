			<st:uint size="3">
				<st:min>0</st:min>
				<st:max>255</st:max>
				<st:default size="3">
					<st:role>Red in RGB order</st:role>
					<st:value>255</st:value>
					<st:value>0</st:value>
					<st:value>0</st:value>
				</st:default>
			</st:uint>
		</st:attribute>
		<st:attribute Name="WithinToleranceColor" alias="WithinToleranceColor" Tabpage="/Shape/FreeStyle/Tolerance">
			<st:role>The Whithin Tolerance Color</st:role>
			<st:uint size="3">
				<st:min>0</st:min>
				<st:max>255</st:max>
				<st:default size="3">
					<st:role>Green in RGB order</st:role>
					<st:value>0</st:value>
					<st:value>255</st:value>
					<st:value>0</st:value>
				</st:default>
			</st:uint>
		</st:attribute>
