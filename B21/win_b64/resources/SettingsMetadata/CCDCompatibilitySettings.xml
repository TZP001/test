<?xml version="1.0" encoding="UTF-8" ?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="CCDCompatibilitySettings" PrimaryTabpage="/General/Compatibility/CCD">
		<st:role>Settings that control the import and export of CCD drawings.</st:role>
		<st:detailedRole>Settings that control the import and export of CCD drawings.</st:detailedRole>
		<st:RepositoryFile>CCDCompatibilitySettings</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
        <st:attribute Name="ImportDftStandard">
            <st:role>Name of the Drafting standard that defines the standard of the resulting Drawing</st:role>
            <st:string>
                <st:default>
                    <st:value>ISO_DS</st:value>
                </st:default>
            </st:string>
        </st:attribute>
        <st:attribute Name="ImportDimensionExplode">
			<st:role>If true, dimension objects from CADAM models will be converted into V5 drawing elements.</st:role>
			<st:boolean>
				<st:default>
					<st:value>false</st:value>
				</st:default>
			</st:boolean>
		</st:attribute>
		<st:attribute Name="ImportSymCatPath">
			<st:role>Directory of CADAM Symbol Tables translated into CATIA V5 Catalogs.</st:role>
			<st:string>
				<st:default>
					<st:value></st:value>
				</st:default>
			</st:string>
		</st:attribute>
        <st:attribute Name="ImportSplineOption">
            <st:role>If true, CADAM Splines are converted to V5 polylines using a linear approximation.</st:role>
            <st:boolean>
                <st:default>
                    <st:value>false</st:value>
                </st:default>
            </st:boolean>
        </st:attribute>
        <st:attribute Name="ImportSplineSagValue">
            <st:role>The sag value in MKS defines the chordal deviation used in the linear approximation.</st:role>
            <st:double>
                <st:default>
                    <st:value>0.00001</st:value>
                </st:default>
            </st:double>
        </st:attribute>
        <st:attribute Name="ExportSplineOption">
            <st:role>If true, V5 curves are converted to CCD polylines using a linear approximation.</st:role>
            <st:boolean>
                <st:default>
                    <st:value>false</st:value>
                </st:default>
            </st:boolean>
        </st:attribute>
        <st:attribute Name="ExportSplineSagValue">
            <st:role>The sag value in MKS defines the chordal deviation used in the linear approximation.</st:role>
            <st:double>
                <st:default>
                    <st:value>0.00001</st:value>
                </st:default>
            </st:double>
        </st:attribute>
        <st:attribute Name="ExportConvertToLineWeights">
            <st:role>V5 line widths are converted to CCD line weights using CCD Style1 mappings.</st:role>
            <st:boolean>
                <st:default>
                    <st:value>true</st:value>
                </st:default>
            </st:boolean>
        </st:attribute>
        <st:attribute Name="LightLineWeight">
            <st:role>Mapping value for CCD light line weight</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>55</st:max>
                <st:default>
                    <st:value>1</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="MediumLineWeight">
            <st:role>Mapping value for CCD medium line weight</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>55</st:max>
                <st:default>
                    <st:value>2</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="HeavyLineWeight">
            <st:role>Mapping value for CCD heavy line weight</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>55</st:max>
                <st:default>
                    <st:value>3</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style1Solid">
            <st:role>V5 line type mapping to CCD style 1 solid line type</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default>
                    <st:value>1</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style1Dash">
            <st:role>V5 line type mapping to CCD style 1 dash line type</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default>
                    <st:value>2</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style1Center">
            <st:role>V5 line type mapping to CCD style 1 center line type</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default>
                    <st:value>4</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style1Phantom">
            <st:role>V5 line type mapping to CCD style 1 phantom line type</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default>
                    <st:value>5</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style1Break">
            <st:role>V5 line type mapping to CCD style 1 break line type</st:role>
            <st:int>
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default>
                    <st:value>8</st:value>
                </st:default>
            </st:int>
        </st:attribute>
        <st:attribute Name="Style2LineTypes">
            <st:role>V5 line type mappings to CCD style 2 line types in an array</st:role>
            <st:int size="19">
                <st:min>1</st:min>
                <st:max>63</st:max>
                <st:default size="19">
                    <!-- SLD -->
                    <st:value>1</st:value>
                    <!-- DSH1 -->
                    <st:value>6</st:value>
                    <!-- DSH2 -->
                    <st:value>2</st:value>
                    <!-- DSH3 -->
                    <st:value>3</st:value>
                    <!-- DSH4 -->
                    <st:value>3</st:value>
                    <!-- DSH5 -->
                    <st:value>3</st:value>
                    <!-- DSH6 -->
                    <st:value>3</st:value>
                    <!-- CL1 -->
                    <st:value>4</st:value>
                    <!-- CL2 -->
                    <st:value>4</st:value>
                    <!-- CL3 -->
                    <st:value>24</st:value>
                    <!-- CL4 -->
                    <st:value>24</st:value>
                    <!-- CL5 -->
                    <st:value>24</st:value>
                    <!-- PL1 -->
                    <st:value>5</st:value>
                    <!-- PL2 -->
                    <st:value>25</st:value>
                    <!-- PL3 -->
                    <st:value>25</st:value>
                    <!-- SBK1 -->
                    <st:value>26</st:value>
                    <!-- SBK2 -->
                    <st:value>34</st:value>
                    <!-- MTCH -->
                    <st:value>5</st:value>
                    <!-- STCH -->
                    <st:value>6</st:value>
                </st:default>
            </st:int>
        </st:attribute>
    </st:attributes>
</st:repository>
