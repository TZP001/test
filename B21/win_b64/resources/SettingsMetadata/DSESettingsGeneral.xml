<?xml version="1.0" encoding="UTF-8" ?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="DSESettingsGeneral" PrimaryTabpage="/Shape/Digitized Shape Editor/General">
		<st:role>Settings to manage Digitized Shape Editor General Options</st:role>
		<st:RepositoryFile>DSESettingsGeneral</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1 </st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="WarmStartNbOfPoints">
			<st:role>Maximum number of points to let the warmstart work properly</st:role>
			<st:int>
				<st:min>0</st:min>
				<st:max>1000000000</st:max>
				<st:default>
					<st:value>1000000</st:value>
				</st:default>
			</st:int>
		</st:attribute>
      <st:attribute Name="UndoActivated">
        <st:role>Toggle to activate/deactivate Undo/Redo for in-place cloud edition commands</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>0</st:value>
          </st:default>
        </st:int>
      </st:attribute>
	</st:attributes>
</st:repository>
