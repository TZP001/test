<?xml version="1.0" encoding="UTF-8"?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:com:dassault_systemes:settings">
  <st:Description name="RenderingSettingsSticker" PrimaryTabpage="/Infrastructure/Real Time Rendering/Sticker" >
    <st:role>Real Time Rendering options</st:role>
    <st:detailedRole>Full explanation of the Setting Role</st:detailedRole>
    <st:RepositoryFile>RenderingOptionsSettings</st:RepositoryFile>
    <st:version>1</st:version>
    <st:compatibility>
      <st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
      <st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
    </st:compatibility>
  </st:Description>

  <st:attributes>

    <st:attribute Name="StkDefaultImage">
      <st:role>Set sticker default image path</st:role>
      <st:string Unicode="true">
        <st:default>
          <st:value></st:value>
        </st:default>
      </st:string>
    </st:attribute>
    
    <st:attribute Name="StickerRealtimeViewMode">
      <st:role>Activate stickers real time representation</st:role>
      <st:boolean StorageType="int">
        <st:default>
          <st:value>true</st:value>
        </st:default>
      </st:boolean>
    </st:attribute>

  </st:attributes>

  <MultiCategoriesTabpages>

    <Tabpage RootTabpage="/Infrastructure/Real Time Rendering/Sticker" ReusedAs="/Infrastructure/Photo Studio/Sticker" />
    
  </MultiCategoriesTabpages>
  
</st:repository>
