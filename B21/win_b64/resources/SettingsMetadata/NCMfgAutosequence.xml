        			<st:default size="1">
          				<st:value>true</st:value>
        			</st:default>
      		</st:boolean>
    		</st:attribute>
    		<st:attribute Name="AuthorizePrioritySet">
      		<st:role>Authorize Priortyset </st:role>
		      <st:detailedRole>Authorize Priorty set</st:detailedRole>
		      <st:boolean size="1">
			      <st:default size="1">
      		    		<st:value>true</st:value>
        			</st:default>
		      </st:boolean>
	    	</st:attribute>
    		<st:attribute Name="ManagingRules">
      		<st:role>ManagingRules </st:role>
      		<st:detailedRole>Managing Auto Sequencing Rules </st:detailedRole>
      		<st:boolean size="1">
     				<st:default size="1">
          				<st:value>true</st:value>
		        	</st:default>
      		</st:boolean>
    		</st:attribute>
