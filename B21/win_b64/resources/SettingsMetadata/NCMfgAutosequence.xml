    		<st:attribute Name="AvailableView">
		      <st:role>Available for view </st:role>
		      <st:detailedRole>Available for view </st:detailedRole>
		      <st:boolean size="1">
			      <st:default size="1">
          				<st:value>true</st:value>
	 	      	</st:default>
		      </st:boolean>
		</st:attribute>
    		<st:attribute Name="AuthorizeRuleSet">
