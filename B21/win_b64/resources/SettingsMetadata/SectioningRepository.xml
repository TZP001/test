<?xml version="1.0" encoding="UTF-8" ?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
	<st:Description name="SectioningRepository" PrimaryTabpage="/Digital Mockup/DMU Space Analysis/DMU Sectioning">
		<st:role>Settings related to the Section command</st:role>
		<st:RepositoryFile>SectioningRepository</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
    <st:attribute Name="PlaneDefaultsColor">
      <st:role>Default color of the section plane</st:role>
      <st:uint size="3">
        <st:min>0</st:min>
        <st:max>255</st:max>
        <st:default size="3">
          <st:role>Green in RGB order</st:role>
          <st:value>255</st:value>
          <st:value>255</st:value>
          <st:value>0</st:value>
        </st:default>
      </st:uint>
    </st:attribute>
    <st:attribute Name="PlaneNormale">
      <st:role>Direction of section plane</st:role>
      <st:uint>
        <st:enum>
          <st:role>X</st:role>
          <st:value>1</st:value>
          <st:role>Y</st:role>
          <st:value>2</st:value>
          <st:role>Z</st:role>
          <st:value>3</st:value>
        </st:enum>
        <st:default>
          <st:role>Default direction of section plane is X</st:role>
          <st:value>1</st:value>
        </st:default>
      </st:uint>
    </st:attribute>
    <st:attribute Name="PlaneOrigine">
      <st:role>Origin of the section plane</st:role>
      <st:int>
        <st:enum>
          <st:role>Origin</st:role>
          <st:value>0</st:value>
          <st:role>Selection</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:role>Default origin is at the selection</st:role>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="PlaneInNoShow">
      <st:role>Hide or show the section plane</st:role>
      <st:int>
        <st:enum>
          <st:role>The plane is hidden</st:role>
          <st:value>0</st:value>
          <st:role>The plane is shown</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="ResultInNoShow">
      <st:role>Hide or show the section result</st:role>
      <st:int>
        <st:enum>
          <st:role>The result is hidden</st:role>
          <st:value>0</st:value>
          <st:role>The result is shown</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="UpdateResult">
      <st:role>Automatically compute the result</st:role>
      <st:int>
        <st:enum>
          <st:role>The result is computed</st:role>
          <st:value>0</st:value>
          <st:role>The result is not computed</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="Wireframe">
      <st:role>Cut wireframe elements</st:role>
      <st:int>
        <st:enum>
          <st:role>Wireframe elements are not cut</st:role>
          <st:value>0</st:value>
          <st:role>Wireframe elements are cut</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="VolumeCutMode">
      <st:role>Allow measure on section</st:role>
      <st:uint>
        <st:enum>
          <st:role>Measure is not allowed</st:role>
          <st:value>0</st:value>
          <st:role>Measure is allowed</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:uint>
    </st:attribute>
    <st:attribute Name="GridLodsDisplay">
      <st:role>Autoamtic grid filtering</st:role>
      <st:int>
        <st:enum>
          <st:role>Grid is not filtered</st:role>
          <st:value>0</st:value>
          <st:role>Grid is filtered</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="GridAutoResize">
      <st:role>Autoamtic grid resizing</st:role>
      <st:int>
        <st:enum>
          <st:role>Grid is not resized</st:role>
          <st:value>0</st:value>
          <st:role>Grid is resized</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="ViewerAutoOpen">
      <st:role>Section viewer window display</st:role>
      <st:int>
        <st:enum>
          <st:role>No autoamtic open</st:role>
          <st:value>0</st:value>
          <st:role>Automatic open</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="ViewerLock2D">
      <st:role>Section in 2D viewer</st:role>
      <st:int>
        <st:enum>
          <st:role>No always 2D view</st:role>
          <st:value>0</st:value>
          <st:role>Always 2D view</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="ViewerReframe">
      <st:role>Section viewer reframe</st:role>
      <st:int>
        <st:enum>
          <st:role>Automatic reframe of the viewer</st:role>
          <st:value>0</st:value>
          <st:role>No automatic reframe of the viewer</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="SectionFilled">
      <st:role>Section Fill</st:role>
      <st:int>
        <st:enum>
          <st:role>Section appears filled</st:role>
          <st:value>0</st:value>
          <st:role>Section does not appear filled</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="SectionExportType">
      <st:role>Section export</st:role>
      <st:int>
        <st:enum>
          <st:role>Use curves for export</st:role>
          <st:value>0</st:value>
          <st:role>Use polyline for export</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="GridAbsoluteMode">
      <st:role>Section grid mode</st:role>
      <st:int>
        <st:enum>
          <st:role>Absolute</st:role>
          <st:value>0</st:value>
          <st:role>Relative</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:role>Default grid mode is absolute</st:role>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="GridLineStyle">
      <st:role>Section grid style</st:role>
      <st:int>
        <st:enum>
          <st:role>Lines</st:role>
          <st:value>0</st:value>
          <st:role>Crosses</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:role>Default grid style is Lines</st:role>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="GridStepLarg">
      <st:role>Section step width</st:role>
      <st:float>
        <st:default>
          <st:role>Default width is 1000 mm</st:role>
          <st:value>100</st:value>
        </st:default>
      </st:float>
    </st:attribute>
    <st:attribute Name="GridStepHaut">
      <st:role>Section step height</st:role>
      <st:float>
        <st:default>
          <st:role>Default height is 1000 mm</st:role>
          <st:value>100</st:value>
        </st:default>
      </st:float>
    </st:attribute>
    <st:attribute Name="TileVertically">
      <st:role>Result window</st:role>
      <st:int>
        <st:enum>
          <st:role>WindowSize</st:role>
          <st:value>0</st:value>
          <st:role>TileVertically</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:role>Default is window size</st:role>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="WindowWidth">
      <st:role>Result window width</st:role>
      <st:int>
        <st:default>
          <st:role>Default width is 200</st:role>
          <st:value>200</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="WindowHeight">
      <st:role>Result window height</st:role>
      <st:int>
        <st:default>
          <st:role>Default height is 200</st:role>
          <st:value>200</st:value>
        </st:default>
      </st:int>
    </st:attribute>
	</st:attributes>
</st:repository>
