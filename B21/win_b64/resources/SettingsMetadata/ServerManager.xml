<?xml version="1.0" encoding="UTF-8"?>
<st:repository xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd">
	<st:Description name="ServerManager" PrimaryTabpage="/General/ServerManager/" ConnectionBehavior="PostConnection">
		<st:role visibility="LA">Server Manager options tabpage</st:role>	
		<st:RepositoryFile>ServerManager</st:RepositoryFile>		
		<st:version>1</st:version>				
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="OneSessionPerUser" alias="OneSessionPerUser" Tabpage="/ServerManager">
			<st:role>Limit to one session per user</st:role>
			<st:detailedRole>When a user logs twice, its first session is cancelled</st:detailedRole>
			<st:int size="1">
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
	</st:attributes>
</st:repository>
