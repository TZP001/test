<?xml version="1.0" encoding="UTF-8"?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <st:Description name="MoldDesignSplit" PrimaryTabpage="/Mechanical Design/Mold Tooling Design/Split" ConnectionBehavior="PostConnection">
    <st:role>Split management</st:role>
    <st:detailedRole>Split management</st:detailedRole>
    <st:RepositoryFile>MoldDesignSplit</st:RepositoryFile>
    <st:version>1</st:version>
    <st:compatibility>
      <st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
      <st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
    </st:compatibility>
  </st:Description>
  <st:attributes>
    <st:attribute Name="CheckReducedSplit">
      <st:role>CheckReducedSplit</st:role>
      <st:detailedRole>CheckReducedSplit</st:detailedRole>
      <st:boolean size="1">
        <st:default size="1">
          <st:value>true</st:value>
        </st:default>
      </st:boolean>
    </st:attribute>
    <st:attribute Name="CoefficientReducedSplit">
      <st:role>CoefficientReducedSplit</st:role>
      <st:detailedRole>CoefficientReducedSplit</st:detailedRole>
      <st:double size="1">
        <st:min>1.01</st:min>
        <st:max>2.0</st:max>
        <st:step>0.01</st:step>
        <st:default>
          <st:value>1.2</st:value>
        </st:default>
      </st:double>
    </st:attribute>
  </st:attributes>
</st:repository>
