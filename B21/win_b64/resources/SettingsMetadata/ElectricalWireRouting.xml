<?xml version="1.0" encoding="UTF-8" ?>
<!--XML file for setting used by ELB application 2010 -->
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="ElectricalWireRouting" PrimaryTabpage="/Equipment &amp; Systems/Electrical Wire Routing/General" >
		<st:role>Define electrical wire routing settings</st:role>
		<st:detailedRole>Define electrical wire routing settings</st:detailedRole>
		<st:RepositoryFile>ElectricalWireRouting</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="ShortName">
			<st:role>Specifies the short name to the favorites list</st:role>
			<st:detailedRole></st:detailedRole>
  			<st:string size="*">
 				<st:default size="3">	
 					<st:value></st:value>
 				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="TypeAttribute">
			<st:role>Specify the type of the attribute in favorites list</st:role>
			<st:string size="*">
				<st:default size="3">
					<st:value></st:value>
				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="TypeElement">
			<st:role>Specify the type of the element in favorites list</st:role>
			<st:string size="*">
				<st:default size="3">
					<st:value></st:value>
				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="NumberAttribute">
			<st:role>Specify the type of the element in favorites list</st:role>
			<st:int StorageType="long">
				<st:default>
					<st:value>3</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="HandleCKESepCode">
			<st:role>Specify the seperation code file/rule</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int StorageType="long">
				<st:enum>
					<st:role>Specify the seperation file path using file browser</st:role>
					<st:value>0</st:value>
					<st:role>No rule</st:role>
					<st:value>1</st:value>
					<st:role>Use of a rule for seperation code</st:role>
					<st:value>2</st:value>
				</st:enum>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="HandleSigSigSepCode">
			<st:role>Compatibility mode</st:role>
			<st:detailedRole>The compatibility between wires or wires and segment can be checked according to this setting</st:detailedRole>
			<st:int StorageType="long">
				<st:enum>
					<st:role>Compatibility between signal-arc is checked</st:role>
					<st:value>0</st:value>
					<st:role>Compatibility between signal is checked</st:role>
					<st:value>1</st:value>
				</st:enum>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="SegregationCount">
			<st:role>Size of the path list  of Segregation file</st:role>
			<st:detailedRole>Size of the path list  of Segregation file</st:detailedRole>
			<st:int StorageType="long">
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="CurrentSegregationIndex">
			<st:role>Index of the selected paths of the Segregation file list </st:role>
			<st:detailedRole>Paths of the Segregation file containing a table of separation code</st:detailedRole>
			<st:int StorageType="long">
				<st:default>
					<st:value>-1</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="RuleText">
			<st:role>Rule used for seperation code</st:role>
			<st:detailedRole></st:detailedRole>
  			<st:string size="*">
 				<st:default size="3">	
 					<st:value></st:value>
 				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="CurrentExtremityColor">
			<st:role>Define extremity color</st:role>
			<st:uint size="3">
				<st:min>0</st:min>
				<st:max>255</st:max>
				<st:default size="3">
					<st:role>Red in RGB order</st:role>
					<st:value>255</st:value>
					<st:value>0</st:value>
					<st:value>0</st:value>
				</st:default>
			</st:uint>
		</st:attribute>
		<st:attribute Name="SegregationTable">
			<st:role>Paths of the Segregation file list </st:role>
			<st:detailedRole>Paths of the Segregation files list containing a table of separation code</st:detailedRole>
			<st:string size="*">
				<st:default></st:default>
		  </st:string>
		</st:attribute>
	</st:attributes>
</st:repository>
