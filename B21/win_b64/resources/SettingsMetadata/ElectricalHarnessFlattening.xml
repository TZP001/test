<?xml version="1.0" encoding="UTF-8" ?>
<!--XML file for setting used by ELB application 2010 -->
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="ElectricalHarnessFlattening" PrimaryTabpage="/Equipment &amp; Systems/Electrical Harness Flattening/General" >
		<st:role>Define electrical harness flattening settings</st:role>
		<st:detailedRole>Define electrical harness flattening settings</st:detailedRole>
		<st:RepositoryFile>ElectricalHarnessFlattening</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="HtmlPath">
			<st:role>HTML path storage</st:role>
			<st:detailedRole>File path where to store the html report generated during the synchronization</st:detailedRole>
			<st:string size="*">
				<st:default></st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="HtmlPathLock">
			<st:role>HTML file path lock</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="CatalogLock">
			<st:role>Drawing catalog lock</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="CatalogArraySize">
			<st:role>Total number of the electrical drawing catalogs</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="ExtractOptionLock">
			<st:role>Extract option lock</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="ExtractOption">
			<st:role>Extract algorithm selection</st:role>
			<st:detailedRole>Extract algorithms - New form and Synchronize</st:detailedRole>
			<st:int>
   				<st:enum>
					<st:role>Use new form algorithm for extract</st:role>
					<st:value>1</st:value>
					<st:role>Use synchronize algorithm for extract</st:role>
					<st:value>2</st:value>
				</st:enum>
				<st:default>
					<st:value>2</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="BNSMasterOption">
			<st:role>3D harness flattening without mechanical constraints between electrical components</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
   				<st:enum>
					<st:role>Use 3d harness flattening with mechanical constraints between electrical components</st:role>
					<st:value>0</st:value>
					<st:role>Use 3d harness flattening without mechanical constraints between electrical components</st:role>
					<st:value>1</st:value>
				</st:enum>
				<st:default>
					<st:value>1</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="NumberingOptionLock">
			<st:role></st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="NumberingOfSection">
			<st:role>2D dress-up settings</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
   				<st:enum>
					<st:role>Keep OFF, the numbering of section when updating dress-up</st:role>
					<st:value>0</st:value>
					<st:role>Keep ON, the numbering of section when updating dress-up</st:role>
					<st:value>1</st:value>
				</st:enum>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="ElectricalConnectSelectOptionLock">
			<st:role>Electrical connection option lock</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
				<st:default>
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="ElectricalConnectSelection">
			<st:role>Electrical connection option</st:role>
			<st:detailedRole></st:detailedRole>
			<st:int>
   				<st:enum>
					<st:role>Do not use electrical connection </st:role>
					<st:value>0</st:value>
					<st:role>Use electrical connection</st:role>
					<st:value>1</st:value>
				</st:enum>
				<st:default>
					<st:value>1</st:value>
				</st:default>
			</st:int>
		</st:attribute>
		<st:attribute Name="CatalogEnvironments">
			<st:role>Calalog environments present</st:role>
			<st:detailedRole></st:detailedRole>
			<st:string size="*">
				<st:default></st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="CatalogPaths">
			<st:role>List of catalog paths</st:role>
			<st:detailedRole></st:detailedRole>
			<st:string size="*">
				<st:default></st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="CatalogNames">
			<st:role>List of catalog names</st:role>
			<st:detailedRole></st:detailedRole>
			<st:string size="*">
				<st:default></st:default>
			</st:string>
		</st:attribute>
	</st:attributes>
</st:repository>
