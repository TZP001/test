<?xml version="1.0" encoding="utf-8"?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <st:Description name="PERTConfig" PrimaryTabpage="/DPManufacturing/Pert Chart" ConnectionBehavior="PostConnection">
    <st:role visibility="Exposed">Tab page to handle the Compare Tool command Setting parameters</st:role>
    <st:RepositoryFile>PERTConfig</st:RepositoryFile>
    <st:version>1</st:version>
    <st:compatibility>
      <st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
      <st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
    </st:compatibility>
  </st:Description>
 
  <st:attributes>
 
    <st:attribute Name="LinkSetting" alias="LinkSetting">
      <st:role>Type Of Link</st:role>
      <st:detailedRole>Type Of Link 0.Straight Link 1. Orthogonal Link</st:detailedRole>
      <st:int size="1">
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>

  <st:attribute Name="IconLabelSetting" alias="IconLabelSetting">
    <st:role>Inside a Pert Node</st:role>
    <st:detailedRole>nside a Pert Node 0.View Icon only 1.View Icon and Node Label</st:detailedRole>
    <st:int size="1">
      <st:default>
        <st:value>1</st:value>
      </st:default>
    </st:int>
  </st:attribute>

  <st:attribute Name="GridShowSetting" alias="GridShowSetting">
    <st:role>Grid</st:role>
    <st:detailedRole>Grid Setting 0.View Grid 1.Hide Grid</st:detailedRole>
    <st:int size="1">
      <st:default>
        <st:value>0</st:value>
      </st:default>
    </st:int>
  </st:attribute>

  <st:attribute Name="MultiPertWndShowSetting" alias="MultiPertWndShowSetting">
    <st:role>Open Pert in</st:role>
    <st:detailedRole>Pert Chart Window Settings. Open Pert Chart in 0.Multiple windows 1.Single Window</st:detailedRole>
    <st:int size="1">
      <st:default>
        <st:value>0</st:value>
      </st:default>
    </st:int>
  </st:attribute>

  <st:attribute Name="DescriptionShowSetting" alias="DescriptionShowSetting">
    <st:role>Description Setting</st:role>
    <st:detailedRole>Mark Activities PertNode if description exists</st:detailedRole>
    <st:int size="1">
      <st:default>
        <st:value>1</st:value>
      </st:default>
    </st:int>
  </st:attribute>

    <st:attribute Name="NodeColorRed" alias="NodeColorRed">
      <st:role>Node Color Red Value</st:role>
      <st:detailedRole>Node Color Red Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.4627</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="NodeColorGreen" alias="NodeColorGreen">
      <st:role>Node Color Green Value</st:role>
      <st:detailedRole>Node Color Green Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.4627</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="NodeColorBlue" alias="NodeColorBlue">
      <st:role>Node Color Blue Value</st:role>
      <st:detailedRole>Node Blue Red Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.4627</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="BackColorRed" alias="BackColorRed">
      <st:role>Background Color Red Value</st:role>
      <st:detailedRole>Background Color Red Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>1.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="BackColorGreen" alias="BackColorGreen">
      <st:role>Background Color Green Value</st:role>
      <st:detailedRole>Background Color Green Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>1.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="BackColorBlue" alias="BackColorBlue">
      <st:role>Background Color Blue Value</st:role>
      <st:detailedRole>Background Blue Red Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>1.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="LinkColorRed" alias="LinkColorRed">
      <st:role>Link Color Red Value</st:role>
      <st:detailedRole>Link Color Red Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="LinkColorGreen" alias="LinkColorGreen">
      <st:role>Link Color Green Value</st:role>
      <st:detailedRole>Link Color Green Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>

    <st:attribute Name="LinkColorBlue" alias="LinkColorBlue">
      <st:role>Link Color Blue Value</st:role>
      <st:detailedRole>Link Color Blue Value</st:detailedRole>
      <st:double size="1">
        <st:default>
          <st:value>0.0</st:value>
        </st:default>
      </st:double>
    </st:attribute>
    
  </st:attributes>
</st:repository>
