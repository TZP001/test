<?xml version="1.0" encoding="UTF-8" ?>
<!--XML file for setting used by ELB application 2010 -->
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="ElectricalLibraryAccess1" PrimaryTabpage="/Equipment &amp; Systems/Electrical Assembly Design/General" >
		<st:role>Electrical Library setting</st:role>
		<st:detailedRole>Electrical Library setting</st:detailedRole>
		<st:RepositoryFile>ElectricalLibraryAccess</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="PlaceCompass">
			<st:role>Place compass on instance after smart move</st:role>
			<st:detailedRole>check to place compass on instance after smart move to refine movement</st:detailedRole>
  			<st:string size="1">
 				<st:default>	
 					<st:value>YES</st:value>
 				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="UseCES">
			<st:role>Enables sourcing access</st:role>
			<st:detailedRole>Enables sourcing access instead of catalogs</st:detailedRole>
  			<st:string size="1">
 				<st:default>
 					<st:value>NO</st:value>
 				</st:default>
			</st:string>
		</st:attribute>
	</st:attributes>
</st:repository>
