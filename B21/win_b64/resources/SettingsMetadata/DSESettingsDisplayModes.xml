<?xml version="1.0" encoding="UTF-8" ?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="DSESettingsDisplayModes" PrimaryTabpage="/Shape/Digitized Shape Editor/Display Modes">
		<st:role>Settings to manage Digitized Shape Editor Display Modes Options</st:role>
		<st:RepositoryFile>DSESettingsDisplayModes</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1 </st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="PointSampling">
			<st:role>Point Sampling</st:role>
      <st:double>
				<st:min>1.0</st:min>
				<st:max>100.0</st:max>
				<st:default>
					<st:value>100.0</st:value>
				</st:default>
			</st:double>
		</st:attribute>
      <st:attribute Name="PointColored">
        <st:role>Toggle to activate/deactivate color on points</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>1</st:value>
          </st:default>
        </st:int>
      </st:attribute>
      <st:attribute Name="PointSymbol">
        <st:role>Toggle to activate/deactivate symbol on points</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>1</st:value>
          </st:default>
        </st:int>
      </st:attribute>    
      <st:attribute Name="PointProtected">
        <st:role>Toggle to activate/deactivate protection on points</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>0</st:value>
          </st:default>
        </st:int>
      </st:attribute>	
      <st:attribute Name="ScanPolyline">
        <st:role>Toggle polylines display</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>1</st:value>
          </st:default>
        </st:int>
      </st:attribute>    
      <st:attribute Name="ScanPoint">
        <st:role>Toggle points display</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>0</st:value>
          </st:default>
        </st:int>
      </st:attribute>	
      <st:attribute Name="ScanOrientation">
        <st:role>Toggle orientations display</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>0</st:value>
          </st:default>
        </st:int>
      </st:attribute>	  
      <st:attribute Name="MeshVertex">
        <st:role>Toggle vertex display</st:role>
        <st:int>
          <st:min>0</st:min>
          <st:max>1</st:max>
          <st:default>
            <st:value>0</st:value>
          </st:default>
        </st:int>
      </st:attribute>
    <st:attribute Name="MeshFreeEdges">
      <st:role>Toggle free edges display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshNonManifoldEdges">
      <st:role>Toggle non-manifold edges display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshNormal">
      <st:role>Toggle normal display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshTriangles">
      <st:role>Toggle triangles display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshShrink">
      <st:role>Toggle shrink display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshFlat">
      <st:role>Toggle flat display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="MeshSmooth">
      <st:role>Toggle smooth display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="LightVisuActivated">
      <st:role>Toggle light visu of meshes display</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="LightVisuNbOfTriangles">
      <st:role>Number of triangles</st:role>
      <st:int>
        <st:min>1</st:min>
        <st:max>1000000000</st:max>
        <st:default>
          <st:value>500000</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="DynamicInfo">
      <st:role>Toggle dynamic display of info</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>0</st:value>
        </st:default>
      </st:int>
    </st:attribute>
    <st:attribute Name="HighlightMode">
      <st:role>Toggle highlight mode</st:role>
      <st:int>
        <st:min>0</st:min>
        <st:max>1</st:max>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
  </st:attributes>
</st:repository>
