<?xml version="1.0" encoding="UTF-8" ?>
<!--XML file for setting used by ELB application 2010 -->
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd" xmlns:st="urn:com:dassault_systemes:settings" xmlns="urn:com:dassault_systemes:settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<st:Description name="CircuitBoardDesign2" PrimaryTabpage="/Equipment &amp; Systems/Circuit Board Design/Components" >
		<st:role>Define circuit board design settings</st:role>
		<st:detailedRole>Define circuit board design settings</st:detailedRole>
		<st:RepositoryFile>CircuitBoardDesign</st:RepositoryFile>
		<st:version>1</st:version>
		<st:compatibility>
			<st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
			<st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
		</st:compatibility>
	</st:Description>
	<st:attributes>
		<st:attribute Name="CatalogPath">
			<st:role>Catalog file path </st:role>
			<st:detailedRole></st:detailedRole>
			<st:string size="*">
				<st:default>
					<st:value> No File </st:value>
				</st:default>
			</st:string>
		</st:attribute>
		<st:attribute Name="ComponentHeigth">
			<st:role>Default Heigth of the component </st:role>
			<st:detailedRole>Default Heigth of the component coming from library file</st:detailedRole>
			<st:double size="1">
				<st:default size="1">
					<st:value>0.1</st:value>
				</st:default>
			</st:double>
		</st:attribute>
		<st:attribute Name="WithConstraint">
			<st:role>Lock the position of the component with constraints </st:role>
			<st:detailedRole>Option to lock the position of the component with constraints</st:detailedRole>
			<st:int>
				<st:enum>
					<st:role>Do not lock the position of the component</st:role>
					<st:value>0</st:value>
					<st:role>Lock the position of the component</st:role>
					<st:value>1</st:value>
				</st:enum>
				<st:default size="1">
					<st:value>0</st:value>
				</st:default>
			</st:int>
		</st:attribute>
	</st:attributes>
</st:repository>
