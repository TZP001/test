<!--

    This is the DTD for printer definition.

-->

<!--================== Imported Names ====================================-->

<!ENTITY % Number "CDATA">
    <!-- one or more digits -->

<!ENTITY % Text "CDATA">
    <!-- used for titles etc. -->


<!--=================== Printer Element ====================================-->
<!-- the element XXXProperties should correspond to the selected export
     except for HPGL2 export that support the HPGL2 properties and the HPGL properties -->

<!ELEMENT printer (defaultFormat?, maxFormat?, defaultMargins?,
                   execUnix?, execWindows?, rtimeUnix?, rtimeWindows?,
                   (CGMSOFTProperties|CGMProperties|RASTERProperties|EPSProperties|
                    HPGL2Properties|HP1000GL2Properties|HPGLProperties|PLUGINProperties|V4Properties)?)>
<!ATTLIST printer
  name %Text; #REQUIRED
  export (CGMSOFT|CGM|RASTER|EMF|EPS|PDF|HPGL2|HP1000GL2|HPGL|SVG|PLUGIN|GBR|OCE) #IMPLIED "EPS">


<!--=================== Printer Properties Elements ====================================-->
<!-- the element defaultFormat will not take into account the attributs width and height if the attribut name is not set to USER -->


<!ELEMENT defaultFormat>
<!ATTLIST defaultFormat 
  name (A0_ISO|A1_ISO|A2_ISO|A3_ISO|A4_ISO|
        A0_ROLL|A1_ROLL|
        A_ANSI|B_ANSI|C_ANSI|D_ANSI|E_ANSI|F_ANSI|
        J_U.S.Standard|
        Letter|Legal|
        B4|B5|C5|
        USER)#IMPLIED "A4_ISO"
  orientation (portrait|landscape) #IMPLIED "portrait"
  width %Number; #IMPLIED
  height %Number; #IMPLIED>

<!ELEMENT maxFormat>
<!ATTLIST maxFormat
  name (A0_ISO|A1_ISO|A2_ISO|A3_ISO|A4_ISO|
        A0_ROLL|A1_ROLL|
        A_ANSI|B_ANSI|C_ANSI|D_ANSI|E_ANSI|F_ANSI|
        J_U.S.Standard|
        Letter|Legal|
        B4|B5|C5|
        USER)#IMPLIED "A4_ISO"
  width %Number; #IMPLIED
  height %Number; #IMPLIED>

<!ELEMENT defaultMargins>
<!ATTLIST defaultMargins
  left %Number; #IMPLIED
  right %Number; #IMPLIED
  top %Number; #IMPLIED
  bottom %Number; #IMPLIED>


<!--=================== Printer Scripts Elements ====================================-->

<!ELEMENT execUnix (#PCDATA)>

<!ELEMENT execWindows (#PCDATA)>

<!ELEMENT rtimeUnix (#PCDATA)>

<!ELEMENT rtimeWindows (#PCDATA)>

<!ELEMENT rtimeWindows (#PCDATA)>


<!--=================== Printer Language Properties Elements ====================================-->


<!--=================== CGMSOFT Properties Element ====================================-->
<!-- about the fontList element, if the discretization attribut is set to YES, the whole fonts will be tesselated
     without taking into account the font lement attributs -->


<!ELEMENT CGMSOFTProperties(version?,profile?,
                            beginMetafile?,beginPicture?,metafileDescription?,
                            VDCType?,scalingMode?,scale?,lineWidthSpecification?,
                            compressionType?,fontList?)>

<!ELEMENT version>
<!ATTLIST version
  value (1|2|3|4) #IMPLIED "3">

<!ELEMENT profile>
<!ATTLIST profile
  value (ISO|ATA|CALS|WEB CGM) #IMPLIED "ISO">

<!ELEMENT beginMetafile>
<!ATTLIST beginMetafile
  value %Text; #IMPLIED>

<!ELEMENT beginPicture>
<!ATTLIST beginPicture
  value %Text; #IMPLIED>

<!ELEMENT metafileDescription>
<!ATTLIST metafileDescription
  value %Text; #IMPLIED>

<!ELEMENT VDCType>
<!ATTLIST VDCType
  value (INT|REAL) #IMPLIED "REAL">

<!ELEMENT scalingMode>
<!ATTLIST scalingMode
  value (ABSTRACT|METRIC) #IMPLIED "METRIC">

<!ELEMENT scale>
<!ATTLIST scale
  value %Number; #IMPLIED>

<!ELEMENT lineWidthSpecification>
<!ATTLIST lineWidthSpecification
  value (ABSOLUTE|SCALED) #IMPLIED "ABSOLUTE">

<!ELEMENT compressionType>
<!ATTLIST compressionType
  value (CCITT Group 4|CCITT Group 3|Uncompressed) #IMPLIED "Uncompressed">

<!ELEMENT fontList (font*)>
<!ATTLIST fontList 
  discretization (YES|NO) #IMPLIED "NO">

<!ELEMENT font>
<!ATTLIST font
  name %Text; #IMPLIED
  action (Discretized|Not Substituted|Substituted) #IMPLIED
  substitution (Courier|Helvetica|Times|Symbol) #IMPLIED "Courier">


