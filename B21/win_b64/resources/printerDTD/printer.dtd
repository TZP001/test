<!--=================== EPS Properties Element ====================================-->

<!ELEMENT EPSProperties>
<!ATTLIST EPSProperties
  level (1|2) #IMPLIED "2"
  encapsulated (YES|NO) #IMPLIED "NO">


<!--=================== HPGL2 Properties Element ====================================-->

<!ELEMENT HPGL2Properties>
<!ATTLIST HPGL2Properties
  mediaType (Default|Paper|Transparency film|Vellum|
             Polyester film|Translucent paper|
             Special paper|Glossy paper) #IMPLIED "Default"
  cutterState (YES|NO) #IMPLIED "YES"
  quality %Number; #IMPLIED "100">


<!--=================== HP1000GL2 Properties Element ====================================-->

<!ELEMENT HP1000GL2Properties ((monoRollFeeder|multiRollFeeder)?)>
<!ATTLIST HP1000GL2Properties
  mediaType (Default|Paper|Transparency film|Vellum|
             Polyester film|Translucent paper|
             Special paper|Glossy paper) #IMPLIED "Default"
  cutterState (YES|NO) #IMPLIED "YES"
  quality (Fast|Normal|Best) #IMPLIED "Best"
  autoRotate (YES|NO) #IMPLIED "YES"
  inkedArea (YES|NO) #IMPLIED "YES"
  ret (YES|NO) #IMPLIED "YES">
 
<!ELEMENT monoRollFeeder>
<!ATTLIST monoRollFeeder
  mediaSource (Roll|ManualFeed) #IMPLIED "Roll">

<!ELEMENT multiRollFeeder>
<!ATTLIST multiRollFeeder
  mediaSource (Roll1|Roll2|Roll3) #IMPLIED "Roll1">


!--=================== HP4000GL2 Properties Element ====================================-->

<!ELEMENT HP4000GL2Properties ((monoRollFeeder|multiRollFeeder)?)>
<!ATTLIST HP4000GL2Properties
  mediaType (Default|Paper|Transparency film|Vellum|
             Polyester film|Translucent paper|
             Special paper|Glossy paper) #IMPLIED "Default"
  cutterState (YES|NO) #IMPLIED "YES"
  printQuality (DRAFT|NORMAL|HIGH) #IMPLIED "NORMAL"
  autoRotate (YES|NO) #IMPLIED "YES"
  inkedArea (YES|NO) #IMPLIED "YES"
  ret (YES|NO) #IMPLIED "YES"
  resolution (300|600|1200) #IMPLIED "600"
  maxDetail (YES|NO) #IMPLIED "YES"
  mediaSource (Roll|ManualFeed) #IMPLIED "Roll">
 

<!--=================== HPGL Properties Element ====================================-->

<!ELEMENT HPGLProperties (pens?)>
<!ATTLIST HPGLProperties
  origin (Center|Lower Left Corner|Upper Left Corner|Lower Right Corner|
             Upper Right Corner) #IMPLIED "Lower Left Corner"
  relativeMovePen (YES|NO) #IMPLIED "YES">
 
<!ELEMENT pens (pen+)>

<!ELEMENT pen>
<!ATTLIST pen
  number %Number; #REQUIRED
  red %Number; #IMPLIED
  green %Number; #IMPLIED
  blue %Number; #IMPLIED
  width %Number; #IMPLIED
  enable (YES|NO) #IMPLIED>


<!--=================== PLUGIN Properties Element ====================================-->

<!ELEMENT PLUGINProperties>
<!ATTLIST PLUGINProperties
  pluginLibrary %Text; #REQUIRED>


<!--=================== V4 Properties Element ====================================-->

<!ELEMENT V4Properties>
<!ATTLIST V4Properties
  customLibrary %Text; #REQUIRED>

