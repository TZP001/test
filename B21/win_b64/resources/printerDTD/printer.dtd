<!ELEMENT profile>
<!ATTLIST profile
  value (ISO|ATA|CALS|WEB CGM) #IMPLIED "ISO">

<!ELEMENT beginMetafile>
<!ATTLIST beginMetafile
  value %Text; #IMPLIED>

<!ELEMENT beginPicture>
<!ATTLIST beginPicture
  value %Text; #IMPLIED>

<!ELEMENT metafileDescription>
<!ATTLIST metafileDescription
  value %Text; #IMPLIED>

<!ELEMENT VDCType>
<!ATTLIST VDCType
  value (INT|REAL) #IMPLIED "REAL">

<!ELEMENT scalingMode>
<!ATTLIST scalingMode
  value (ABSTRACT|METRIC) #IMPLIED "METRIC">

<!ELEMENT scale>
<!ATTLIST scale
  value %Number; #IMPLIED>

<!ELEMENT lineWidthSpecification>
<!ATTLIST lineWidthSpecification
  value (ABSOLUTE|SCALED) #IMPLIED "ABSOLUTE">

<!ELEMENT compressionType>
<!ATTLIST compressionType
  value (CCITT Group 4|CCITT Group 3|Uncompressed) #IMPLIED "Uncompressed">

<!ELEMENT fontList (font*)>
<!ATTLIST fontList 
  discretization (YES|NO) #IMPLIED "NO">

<!ELEMENT font>
<!ATTLIST font
  name %Text; #IMPLIED
  action (Discretized|Not Substituted|Substituted) #IMPLIED
  substitution (Courier|Helvetica|Times|Symbol) #IMPLIED "Courier">


<!--=================== SVG Properties Element ====================================-->
<!-- about the fontList element, if the discretization attribut is set to YES, the whole fonts will be tesselated
     without taking into account the font lement attributs -->


<!ELEMENT SVGProperties (imageStorage?,fontList?)>

<!ELEMENT version>
<!ATTLIST version
  value (EXTERNAL|EMBED) #IMPLIED "EXTERNAL">

<!ELEMENT fontList (font*)>
<!ATTLIST fontList 
  discretization (YES|NO) #IMPLIED "YES">

<!ELEMENT font>
<!ATTLIST font
  name %Text; #IMPLIED
  action (Discretized|Not Substituted|Substituted) #IMPLIED
  substitution (Times New Roman|Arial|Courier New) #IMPLIED "Times New Roman">


<!--=================== Raster Properties Element ====================================-->
<!-- the TIFFProperties element will not take into account the quality attribut if the compression attribut is set to JPEG -->

<!ELEMENT RASTERProperties ((TIFFProperties|BMPProperties|PCXProperties|TGAProperties|
                             WPGProperties|WMFProperties|DCXProperties|JPEGProperties|
                             PICTProperties|CALSProperties|LASER DATAProperties|XBMProperties|
                             MACProperties|KOFAXProperties|IOCAProperties|ICONProperties|
                             ILBMProperties|CLIPProperties|MSPProperties|XPMProperties|
                             XWDProperties|RASTProperties|PHOTOSHOPProperties|IMNETProperties|
                             PNGProperties)?)>

<!ELEMENT TIFFProperties>
<!ATTLIST TIFFProperties
  dpi %Number; #IMPLIED
  compression (No compression|Packbits|JPEG|HUFFMAN|Fax Group 4) #IMPLIED
  quality %Number; #IMPLIED>

<!ELEMENT BMPProperties>
<!ATTLIST BMPProperties
  dpi %Number; #IMPLIED
  compression (No compression|RLE compression) #IMPLIED>

<!ELEMENT PCXProperties>
