"@@Pervasives@Orderable" ->  "@@Pervasives@float64" ;
"@@Pervasives@Number" ->  "@@Pervasives@float64" ;
"@@Pervasives@Floating" ->  "@@Pervasives@float64" ;
"@@Pervasives@Integer" ->  "@@Pervasives@int32" ;
"@@Pervasives@String" ->  "@@Pervasives@string8" ;
"@@Pervasives@Char" ->  "@@Pervasives@char8" ;
"@@Pervasives@Word" ->  "@@Pervasives@word32" ;
   
