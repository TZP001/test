document `CATDesignerFeaturesSU.feat` {
  container CATFeatCont #root {
    catalog_manager `MechMod.feat`
    catalog_manager `CATHybridShape.feat`

	feature CATDesMonoDimFeature GSMGeom@`CATHybridShape.feat` #startup {					// Base startup for mono-dimensional features
    }
    
    feature CATDesBiDimFeature GSMGeom@`CATHybridShape.feat` #startup {						// Base startup for bi-dimensional features
    }
    
    // Base startur for operation
    feature CATDesOperation GSMGeom@`CATHybridShape.feat` #startup {
    }
    
    feature CATDesSubdivFeatureName CATDesBiDimFeature #startup #backup_startup(FeatureBackUpGeoElem3D@`MechMod.feat`) #isa(CATDesSubdivisionFeature) { // Startup for the main subdivision feature
        specobject SubdivGeomFeature                #neutral                                  // Attribute to stream the mesh
        specobject AssociationFeature        #list  #in                                       // Attribute to store an association feature
    }
    
    feature CATDesSubdivGeomFeatureName CATDesBiDimFeature #startup #backup_startup(FeatureBackUpGeoElem3D@`MechMod.feat`) #isa(CATDesSubdivGeomFeature) { // Startup for the subdivGeom feature (stream of the mesh)
    }
    
    feature CATDesAssociationFeatureName #startup #isa(CATDesAssociationFeature) {			  // Startup for the association feature of subdiv.
        specobject AssociatedFeature                #in                                       // Attribute to store the associated feature
        int        AssociatedVertexNumber    #list  #neutral                                  // Attribute to store the number of associated vertices
        double     AssociatedParamT          #list  #neutral                                  // Attribut to store the curve parameters
        double     AssociatedParamUVW        #list  #neutral                                  // Attribut to store the vertices coordinates
    }
    
    feature CATDesSymmetryFeatureName CATDesOperation #startup #backup_startup(FeatureBackUpGeoElem3D@`MechMod.feat`) #isa(CATDesSymmetryFeature) {     // Startup for the symmetry feature.
        specobject SubdivisionFeature               #in                                       // Attribute to store the subdiv. that will be used.
        specobject PlaneFeature                     #in                                       // Attribute to store the plane of symmetry
        int        DirectionSpec                    #in                                       // Attribute to store the direction of the symmetry
        specobject SubdivGeomFeature                #neutral                                  // Attribute to stream the resulting mesh
        int        MiddleEdgeWeightValue            #in										  // Attribute to stream the value of the weight (middle edge)   
        
    }
    feature CATDesGlobalTransfoFeatureName CATDesOperation #startup #backup_startup(FeatureBackUpGeoElem3D@`MechMod.feat`) #isa(CATDesGlobalTransfoFeature) {     // Startup for the Global Transformation feature.
        specobject SubdivisionFeature               #in                                       // Attribute to store the subdiv. that will be used.
        double     TransfoParameters    			#list  #in                                // Attribut to store the transformation parameters
        specobject SubdivGeomFeature                #neutral                                  // Attribute to stream the resulting mesh
    }
  }
}
