// Create a new type 					
class InComponent component {					
   #in = true // set quality as in					
}

class MfgParameter_Behavior { 					
	// AddStrategyParameter : Create a new strategy parameter					
	   def AddStrategyParameter(iAttrName,iLiteralType,iDefaultValue) { 					   				
	   // Valuation of the new component with a cke parm, values are given by the input parameters								
	   add_key(InComponent,iAttrName, new_literal(iLiteralType,iDefaultValue) )				
	   } 
	   
	  // AddFeedrateParameter : Create a new Feedrate parameter	and add it in a MfgParameter				
	   def AddFeedrateParameter(iAttrName,iFeedrateParam) { 					   				
	   // Valuation of the new component with a MfgFeedrateParameter								
	   add_key(InComponent,iAttrName, iFeedrateParam)				   				
	} 
	
}
					
