ANSI_B18_3_3M_HEXAGON_SOCKET_HEAD_SHOULDER_SCREWS    = "NO,   COMPUTE:FORMULA_4, A_head_dia_max,        D1_Nom_thread_Size,  E_thread_len_max,   D_shoulder_dia_max,  L_length_nom,       pitch";
