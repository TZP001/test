ANSI_B18_3_1M_HEX_SOCKET_HEAD_CAP_SCREWS             = "NO,   L_length_nom,      A_head_dia_max,        D_body_dia_max,      COMPUTE:FORMULA_2,  D_body_dia_max,      Lb_Length,          pitch";
