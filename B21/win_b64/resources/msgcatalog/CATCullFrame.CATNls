CullFrame.HeaderFrame.Global.Title = "Pixel Culling";

CullFrame.IconAndOptionsFrame.OptionsFrame.StaticCullLabel.Title = "Static";
CullFrame.IconAndOptionsFrame.OptionsFrame.StaticCullLabel.ShortHelp = 
"Defines minimum pixel's size to display an object.";
CullFrame.IconAndOptionsFrame.OptionsFrame.StaticCullSlider.LongHelp = 
"Adds or removes display quality for static objects.
Even if you do not want to move geometry, it is often 
useful to remove details you do not need to see. 
Set a low value if you want to see all the details, 
or a high value to remove details.";
CullFrame.IconAndOptionsFrame.OptionsFrame.StaticCullSlider.ShortHelp = "0 - 40";

CullFrame.IconAndOptionsFrame.OptionsFrame.DynamicCullLabel.Title = "While Moving";
CullFrame.IconAndOptionsFrame.OptionsFrame.DynamicCullLabel.ShortHelp = 
"Defines minimum pixel's size to display an object when moving.
High value = moving large parts more quickly.";
CullFrame.IconAndOptionsFrame.OptionsFrame.DynamicCullSlider.LongHelp = 
"Adds or removes display quality when moving objects.
You will be able to move large parts more quickly if 
you set While Moving to a high value. When you release 
the mouse after moving the part, the static size will be redisplayed.";
CullFrame.IconAndOptionsFrame.OptionsFrame.DynamicCullSlider.ShortHelp = "0 - 40";
