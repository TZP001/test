glowFrame.HeaderFrame.Global.Title = "Glow";

glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowCheck.Title = "Active";
glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowCheck.Help = "Switches glow";
