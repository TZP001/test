glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdLabel.Title = "Threshold:";
glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdLabel.Help = "Glow threshold";
glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdLabel.ShortHelp = "Glow Threshold";
glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdLabel.LongHelp =
"Defines the glow threshold. 
A pixel with a color component (red, green, or blue)
above that threshold will be glowed.
This threshold determines how many pixels from
the image will be processed for the glow effect.
Note that a high number of pixels to process may 
increase dramatically the processing time.";

glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdSpinner.Help = "Glow threshold";
glowFrame.IconAndOptionsFrame.OptionsFrame.glowParametersFrame.glowThresholdSpinner.ShortHelp = "Glow Threshold";
