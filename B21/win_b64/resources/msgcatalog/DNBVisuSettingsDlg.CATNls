Title = "Visualization Settings";
BApply = "Reset";
LODFrame.Title = "Level of detail";
CullFrame.Title = "Pixel Culling";

LODFrame.stLODLabel.Title = "Static";
LODFrame.stLODLabel.ShortHelp = "Defines display quality for static objects.
Low value = show all the details.";
LODFrame.stLODLabel.LongHelp = "Adds/removes display quality for static objects.
Even if you do not want to move geometry, it is often 
useful to remove details you do not need to see. 
Set a low value if you want to see all the details, 
or a high value to remove details.";
LODFrame.stLODSlider.Title = "Static LOD";
LODFrame.stLODSlider.ShortHelp = "0.0 - 40.0";
LODFrame.stLODSlider.LongHelp = "Allows the selection of the static LOD value between 0.0 - 40.0";

LODFrame.dynLODLabel.Title = "While moving";
LODFrame.dynLODLabel.ShortHelp = "Defines display quality when moving objects.
High value = moving large parts faster.";
LODFrame.dynLODLabel.LongHelp = "Adds/removes display quality when moving objects.
You will be able to move large parts faster if 
you set While Moving to a high value. When you release 
the mouse after moving the part, the static level of 
detail will be redisplayed.";
LODFrame.dynLODSlider.Title = "Dynamic LOD";
LODFrame.dynLODSlider.ShortHelp = "0.0 - 40.0";
LODFrame.dynLODSlider.LongHelp = "Allows the selection of the dynamic LOD value between 0.0 - 40.0";

CullFrame.stCullLabel.Title = "Static";
CullFrame.stCullLabel.ShortHelp = "Defines minimum pixel's size to display an object.";
CullFrame.stCullLabel.LongHelp = "Adds or removes display quality for static objects.
Even if you do not want to move geometry, it is often 
useful to remove details you do not need to see. 
Set a low value if you want to see all the details, 
or a high value to remove details.";
CullFrame.stCullSlider.Title = "Static Pixel Culling";
CullFrame.stCullSlider.ShortHelp = "0 - 40";
CullFrame.stCullSlider.LongHelp = "Allows the selection of the static Pixel Culling value between 0 - 40";

CullFrame.dynCullLabel.Title = "While moving";
CullFrame.dynCullLabel.ShortHelp = "Defines minimum pixel's size to display an object when moving.
High value = moving large parts more quickly.";
CullFrame.dynCullLabel.LongHelp = "Adds or removes display quality when moving objects.
You will be able to move large parts more quickly if 
you set While Moving to a high value. When you release 
the mouse after moving the part, the static size will be redisplayed.";
CullFrame.dynCullSlider.Title = "Dynamic Pixel Culling";
CullFrame.dynCullSlider.ShortHelp = "0 - 40";
CullFrame.dynCullSlider.LongHelp = "Allows the selection of the dynamic Pixel Culling value between 0 - 40";

DNBVisSetErrorTitleId = "Warning";
