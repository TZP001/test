SelectionFrame.IconAndOptionsFrame.OptionsFrame.BBSelCheckButton.Title      = "Display manipulation bounding box";
SelectionFrame.IconAndOptionsFrame.OptionsFrame.BBSelCheckButton.ShortHelp   = 
"Activates the display of the manipulation bounding box 
