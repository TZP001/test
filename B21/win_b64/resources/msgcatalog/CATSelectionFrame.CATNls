SelectionFrame.HeaderFrame.Global.Title = "Selection";

SelectionFrame.IconAndOptionsFrame.OptionsFrame.PreSelCheckButton.Title = "Preselect in geometry view";
SelectionFrame.IconAndOptionsFrame.OptionsFrame.PreSelCheckButton.LongHelp   =
"Activates preselection highlighting. As you point to objects, 
