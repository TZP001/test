FrameName.Name.Help = "Den Parameter umbenennen";
FrameName.Name.LongHelp = "Der ausgewählte Parameter kann umbenannt werden.\nBei Eingabe des Namens für den Parameter folgende Regeln beachten: \n- keine Leerzeichen im Namen\n- keine der folgenden Zeichen im Namen: #, /, +, -, *,  \, (, ), {, }, [, ], <, >\n- keine Schlüsselwörter (z. B. if, then, else, or, and)";

CATModifyParamFrame.Error.Title  = "Fehler";
