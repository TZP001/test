Title = "Werkzeug/Rohteil";

M3xStopPosition = "Position";
M3xHardStopOffset = "Offset (% Ø)";
M3xStockOvershoot = "Überreichweite";
M3xOvershootTolerance = "Zu bearbeitende Mindeststärke";

M3xStopPosition.LongHelp = "Gibt an, wo der Mittelpunkt des Werkzeugs stoppt:
- 'außerhalb': Das Werkzeug stoppt außerhalb des Rohteils.
- 'innerhalb': Das Werkzeug stoppt innerhalb des Rohteils.
- 'auf': Das Werkzeug stoppt auf dem Rohteil.";

M3xHardStopOffset.LongHelp = "Gibt die Entfernung an, um die das Werkzeug sich über die Position hinaus bewegen darf.  Diese wird in Prozent (Prozentsatz des Werkzeugdurchmessers) ausgedrückt.";

M3xStockOvershoot.LongHelp = "Gibt an, ob das Werkzeug über das Rohteil
hinausgehen darf.";

M3xOvershootTolerance.LongHelp = "Gibt die Mindeststärke des Materials an, das entfernt wird. Diese Stärke wird in der X-,Y- oder Z-Richtung verwendet";
