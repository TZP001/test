Title = "Stratégie d'usinage";

M3xMachiningMode = "Mode d'usinage";
M3xMachiningMode.LongHelp = "Définit le mouvement de l'outil. Les modes possibles sont les suivants :
- Aller simple : le trajet conserve toujours la même direction
    et relie directement la fin d'une passe et la passe suivante,
- Aller+retour : le trajet conserve toujours la même direction
    mais on repasse par le début de chaque passe avant
    d'atteindre le premier point de la passe suivant,
- Zig-zag : le trajet change de direction à chaque passe d'outil. ";

MachiningTolerance = "Tolérance d'usinage";

MachiningTolerance.LongHelp = "Définit l'erreur maximale existant 
entre le trajet outil et le trajet théorique correspondant.";


M3xReverseTP = "Inverser le trajet d'outil";
M3xReverseTP.LongHelp = "Commence la trajectoire de l'outil depuis l'extrémité opposée.";

M3xWithMaxDiscretization = "Discrétisation Max";
M3xWithMaxDiscretization.LongHelp = "Ajoute des points de discrétisation sur les zones planes.";

M3xStepDiscretization = "Pas";
M3xStepDiscretization.LongHelp = "Définit le pas de discrétisation maximale";

M3xDistributionMode = "Mode de Distribution";
M3xDistributionMode.LongHelp = "Définit le mode de distribution des points de discrétisation";

M3xPlungeMode = "Mode de plongée";
M3xPlungeMode.LongHelp = "Définit le mode de plongée. Ce mode peut être :
- Sans Vérification : l'outil peut plonger et remonter en suivant la surface,
- Sans plongée : l'outil peut plonger,
- Hauteur constante : l'outil ne plonge pas mais ne s'arrête pas en rencontrant un pic.";
