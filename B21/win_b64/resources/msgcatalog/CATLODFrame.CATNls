LODFrame.HeaderFrame.Global.Title = "Level of detail";

LODFrame.IconAndOptionsFrame.OptionsFrame.StaticLabel.Title = "Static";
LODFrame.IconAndOptionsFrame.OptionsFrame.StaticLabel.ShortHelp = 
"Defines display quality for static objects.
Low value = show all the details.";
LODFrame.IconAndOptionsFrame.OptionsFrame.StaticSlider.LongHelp = 
"Adds or removes display quality for static objects.
Even if you do not want to move geometry, it is often 
useful to remove details you do not need to see. 
Set a low value if you want to see all the details, 
or a high value to remove details.";
LODFrame.IconAndOptionsFrame.OptionsFrame.StaticSlider.ShortHelp = "0.0 - 40.0";

LODFrame.IconAndOptionsFrame.OptionsFrame.DynamicLabel.Title = "While Moving";
LODFrame.IconAndOptionsFrame.OptionsFrame.DynamicLabel.ShortHelp = 
"Defines display quality when moving objects.
High value = moving large parts more quickly.";
LODFrame.IconAndOptionsFrame.OptionsFrame.DynamicSlider.LongHelp = 
"Adds or removes display quality when moving objects.
You will be able to move large parts more quickly if 
you set While Moving to a high value. When you release 
the mouse after moving the part, the normal level of 
detail will be redisplayed.";
LODFrame.IconAndOptionsFrame.OptionsFrame.DynamicSlider.ShortHelp = "0.0 - 40.0";
