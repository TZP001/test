Title = "Mesh Morphing";

MainFrame.MeshToDeformLabel.Title="Mesh to Deform:" ;
MainFrame.MeshToDeformLabel.LongHelp="Displays the mesh to deform." ;

MainFrame.LimitElementLabel.Title="Limit Element:" ;
MainFrame.LimitElementLabel.LongHelp="Displays the curve or scan which limits the mesh area to deform." ;

MainFrame.TargetLabel.Title="Target Elements:" ;
MainFrame.TargetLabel.LongHelp="Displays target points and curves through which the deformed mesh passes." ;

MainFrame.DeformatinLawLabel.Title="Constraint Attenuation:" ;
MainFrame.DeformatinLawLabel.LongHelp="Defines the degree of constraint attentuation between the limit element and target elements." ;

DeformationLaw.Degree0="Low";
DeformationLaw.Degree1="Medium";
DeformationLaw.Degree2="Strong";
DeformationLaw.Degree3="Very Strong";

MainFrame.InsertTargetCheckB.Title="Insert Target" ;
MainFrame.InsertTargetCheckB.LongHelp="Inserts targets inside the mesh to deform." ;

MainFrame.InsertLimitCheckB.Title="Insert Limit" ;
MainFrame.InsertLimitCheckB.LongHelp="Inserts limit inside the mesh to deform." ;

MainFrame.ProjFrame.Title="Projection Type" ;
MainFrame.ProjFrame.LongHelp="Displays the projection type. The target elements are projected accordingly 
onto the mesh to deform to obtain a reference element. The mesh is deformed as if the reference
element is dragged to the target elements." ;

MainFrame.ProjFrame.NormalRadio.Title="Normal" ;
MainFrame.ProjFrame.NormalRadio.LongHelp="Projects the target elements orthogonally onto the mesh to deform." ;

MainFrame.ProjFrame.DirectionRadio.Title="Direction" ;
MainFrame.ProjFrame.DirectionRadio.LongHelp="Projects the target elements onto the mesh to deform along a given direction." ;

MainFrame.ProjFrame.DirectionLabel.Title="Direction:" ;
MainFrame.ProjFrame.DirectionLabel.LongHelp="Defines projection direction if the projection is of direction type." ;
