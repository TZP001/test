	description=Macromedia Flash; \
	file_extensions=.swf

image/jpeg:\
	description=JPEG image format;\
	file_extensions=.jpg,.jpeg

image/png:\
	description=PNG image format;\
	file_externsions=.png

misc/real:\
	description=RealPlayer files;\
	file_extensions=.rm,.ra,.ram,.smi,.rt
 


