

application/vnd.wap.wmlc: \
	description=WAP WML binary;\
	file_extensions=.wmlc

text/vnd.wap.wml: \
	description=WAP WML text;\
	file_extensions=.wml

text/text:
	description=Pure Ascii Text;\
	file_extensions=.txt
	
